{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next",
        "installCommand": "npm install --include=dev",
        "buildCommand": "npm install typescript @types/react @types/node @types/react-dom --no-save && next build"
      }
    }
  ]
}
