{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": ".next"
      }
    }
  ],
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install --include=dev",
  "framework": "nextjs",
  "outputDirectory": ".next"
}
