{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next",
        "installCommand": "npm install --include=dev",
        "buildCommand": "chmod +x ./scripts/build.sh && ./scripts/build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "buildCommand": "npm install typescript @types/react @types/node @types/react-dom && next build",
  "installCommand": "npm install",
  "framework": "nextjs"
}
